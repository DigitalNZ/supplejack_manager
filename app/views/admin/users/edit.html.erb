<h2><%= @user['id'] %></h2>

<%= form_for :user,
             url: environment_admin_user_path(environment: params[:environment],
             id: @user['id']),
             method: :patch,
             html: { id: "edit-user-form-#{@user['id']}" } do |f| %>
  <input type='text' name='max_requests' value="<%= @user['max_requests'] %>" />
  <%= f.submit class: 'button' %>
<% end %>
